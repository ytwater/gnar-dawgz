{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "gnar-dawgs",
	"compatibility_date": "2025-08-03",
	"compatibility_flags": ["nodejs_compat"],
	"main": "./workers/app.ts",
	"routes": [
		{
			"pattern": "www.gnardawgs.surf",
			"custom_domain": true
		}
	],
	"vars": {
		"ENVIRONMENT": "prod",
		"VALUE_FROM_CLOUDFLARE": "Hello from Cloudflare",
		"ACCOUNT_ID": "fa192b38975cc1a093d7dc819afa0c64",
		"AI_GATEWAY_ID": "default",
		"GOOGLE_CLIENT_ID": "843872966928-4q8r4o4p52h90f52jpjotmmncgr5mrra.apps.googleusercontent.com",
		"VAPID_PUBLIC_KEY": "BLesCxd24BjJTGhGCLKN_b9mOCwlyL-paNFAyCMybZb5UbQ854bSVNV9zgCZJPNFBYwusXwFQTEKhq8pWAR_LYM",
		"VAPID_SUBJECT": "https://gnardawgs.surf",
		"TWILIO_SERVICE_SID": "ISc485decce355478e8ddb9b82dec4ccaa",
		"TWILIO_EVENT_SYNC_ID_DEV": "DGbd9f47b1c0b9e4f7ce4222a2fdb9bfc2",
		"TWILIO_EVENT_SYNC_ID_PROD": "DF2ebba80cc2694b7d743dcc6b346bebec"
	},
	"observability": {
		"enabled": true
	},
	"durable_objects": {
		"bindings": [
			{
				"name": "Chat",
				"class_name": "Chat"
			}
		]
	},
	"migrations": [
		{
			"tag": "v1",
			"new_sqlite_classes": ["Chat"]
		}
	],
	"d1_databases": [
		{
			"binding": "DB",
			"database_name": "gnar-dawgs-db",
			"database_id": "e29a4bfe-8158-4da1-a06a-8ed1cd4e4913",
			"migrations_dir": "./drizzle"
		}
	],
	"kv_namespaces": [
		{
			"binding": "KV",
			"id": "e19b719f2e4f4ad49afb10ab1392a87f"
		}
	],
	"queues": {
		"producers": [
			{
				"binding": "WHATSAPP_QUEUE",
				"queue": "gnar-dawgs-whatsapp-queue"
			}
		],
		"consumers": [
			{
				"queue": "gnar-dawgs-whatsapp-queue",
				"max_batch_size": 5,
				"max_batch_timeout": 5,
				"max_retries": 3
			}
		]
	},
	"triggers": {
		"crons": ["0 */1 * * *"]
	}
}
