{
	"name": "my-react-router-app",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "run-s build:app build:api",
		"build:app": "react-router build",
		"build:api": "orval --config orval.config.cjs",
		"cf-typegen": "wrangler types --no-include-runtime --env-interface CloudflareBindings",
		"cf-typegen:full": "wrangler types --env-interface CloudflareBindings",
		"deploy": "pnpm run build && wrangler deploy",
		"logs": "wrangler tail",
		"dev": "react-router dev",
		"postinstall": "npm run cf-typegen",
		"preview": "pnpm run build && vite preview",
		"typecheck": "npm run cf-typegen && react-router typegen && tsc -b",
		"auth:generate": "npx @better-auth/cli@latest generate --config ./app/lib/auth.ts --output ./app/lib/auth-schema.ts -y",
		"auth:format": "biome lint --write ./app/lib/auth-schema.ts",
		"auth:update": "pnpm run auth:generate && pnpm run auth:format",
		"drizzle-kit-version": "drizzle-kit --version",
		"db:generate": "drizzle-kit generate",
		"db:migrate:dev": "wrangler d1 migrations apply DB --local",
		"db:migrate:prod": "wrangler d1 migrations apply DB --remote",
		"db:studio:dev": "drizzle-kit studio --config drizzle.config.development.ts",
		"db:studio:prod": "drizzle-kit studio --config drizzle.config.production.ts",
		"db:nuke:dev": "npx tsx scripts/nuke.ts development",
		"db:nuke:prod": "npx tsx scripts/nuke.ts production",
		"db:drop:dev": "npx tsx scripts/drop.ts development",
		"db:drop:prod": "npx tsx scripts/drop.ts production"
	},
	"dependencies": {
		"@ai-sdk/deepseek": "^1.0.31",
		"@ai-sdk/openai": "^2.0.77",
		"@ai-sdk/react": "^2.0.106",
		"@ai-sdk/ui-utils": "^1.2.11",
		"@orpc/client": "^1.13.0",
		"@orpc/server": "^1.13.0",
		"@phosphor-icons/react": "^2.1.10",
		"@tanstack/react-query": "^5.90.12",
		"@twilio/conversations": "^2.6.5",
		"agents": "^0.2.29",
		"ai": "^5.0.106",
		"better-auth": "^1.4.7",
		"better-auth-cloudflare": "^0.2.9",
		"clsx": "^2.1.1",
		"drizzle-orm": "^0.45.1",
		"isbot": "^5.1.27",
		"marked": "^17.0.1",
		"openai": "^6.10.0",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-markdown": "^10.1.0",
		"react-router": "^7.11.0",
		"remark-gfm": "^4.0.1",
		"tailwind-merge": "^3.4.0",
		"jose": "^6.1.0",
		"webpush-webcrypto": "^1.0.5",
		"zod": "^4.1.13"
	},
	"devDependencies": {
		"@better-auth/cli": "^1.4.7",
		"@cloudflare/vite-plugin": "^1.17.1",
		"@dotenvx/dotenvx": "^1.51.2",
		"@react-router/dev": "^7.7.1",
		"@react-router/fs-routes": "^7.11.0",
		"@tailwindcss/vite": "^4.1.4",
		"@types/node": "^20",
		"@types/react": "^19.1.2",
		"@types/react-dom": "^19.1.2",
		"better-sqlite3": "^12.5.0",
		"drizzle-kit": "^0.31.8",
		"npm-run-all": "^4.1.5",
		"orval": "^7.17.2",
		"readline": "^1.3.0",
		"tailwindcss": "^4.1.4",
		"typescript": "^5.9.3",
		"vite": "^6.3.3",
		"vite-plugin-node-polyfills": "^0.24.0",
		"vite-tsconfig-paths": "^5.1.4",
		"wrangler": "^4.56.0"
	}
}